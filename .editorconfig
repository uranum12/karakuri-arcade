root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.md]
indent_size = 4
indent_style = space
trim_trailing_whitespace = false

[*.{c,h}]
indent_size = 4
indent_style = space
trim_trailing_whitespace = true

[{CMakeLists.txt,*.cmake}]
indent_size = 4
indent_style = space
trim_trailing_whitespace = true

